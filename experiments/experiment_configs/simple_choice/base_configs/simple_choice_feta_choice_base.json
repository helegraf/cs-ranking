{
  "resources": {
    "gtx1080": "1",
    "ncpus": "2",
    "vmem": "42g",
    "mem": "16g"
  },
  "dataset_configs": [
    {
      "dataset": "synthetic_choice",
      "dataset_params": {
        "dataset_type": "knapsack",
        "n_objects": 10,
        "capacity": 55,
        "n_train_instances": 102400,
        "n_test_instances": 10240
      }
    }
  ],
  "folds": [
    {
      "fold_id": 1,
      "n_inner_folds": 0
    }
  ],
  "learning_problem": "choice_function",
  "seeds": [
    {
      "seed": 5678
    }
  ],
  "learners": [
    {
      "learner_name": "feta_choice",
      "learner_params": {
        "attention_pooling": {
          "PMA": {
            "k": 1,
            "mab": {
              "MAB": {
                "multi_head": {
                  "ScaledDotProductAttention": {"weighted": false, "biased": false}
                }
              }
            },
            "depth_rff": 1,
            "rff_config": {
              "units": 5,
              "activation": "relu",
              "kernel_initializer": "lecun_normal",
              "use_bias": true,
              "bias_initializer": "zeros"}
          }
        },
        "attention_preselection_config": null,
        "attention_pooling_flavour": "mab",
        "num_attention_preselection_layers": 1,
        "loss_function": "binary_crossentropy",
        "loss_function_requires_x_values": false,
        "n_hidden": 2,
        "n_units": 5,
        "add_zeroth_order_model":true,
        "max_number_of_objects": 5,
        "n_hidden_joint_layers": 3,
        "n_hidden_joint_units": 5,
        "activation": "selu",
        "kernel_initializer": "lecun_normal",
        "batch_size": 256,
        "batch_normalization": false,
        "kernel_regularizer": "l2",
        "kernel_regularizer_params": {
          "l": 1e-4
        },
        "optimizer": "SGD",
        "optimizer_params": {
          "lr": 1e-4,
          "nesterov": "true",
          "momentum": 0.9
        }
      },
      "learner_fit_params": {
        "epochs": 64,
        "validation_split": 0.1,
        "tune_size": 0.1,
        "thin_thresholds": 1,
        "callbacks": {
            "AdvancedTensorBoard": {
              "num_visualizations_per_epoch": 1,
              "save_space": true,
              "log_lr": true,
              "log_gradient_norms": "global",
              "log_attention": true,
              "no_detailed_histograms": true,
              "lr_vs_accuracy_diagram": true,
              "log_lr_each_batch": true,
              "log_dir": "./tensorboard_logs/",
              "histogram_freq": 1,
              "write_graph": false,
              "write_grads": false,
              "write_images": false,
              "embeddings_freq": 0,
              "update_freq": "epoch"
          },
          "CyclicLR": {
            "base_lr": 1e-10,
            "max_lr": 1.0,
            "step_size": 25600,
            "mode": "triangular2"
          }
        }
      }
    }
  ],
  "optimizers": [
    {
      "use_hp": "False",
      "hp_iterations": 0,
      "hp_ranges": {},
      "hp_fit_params": {}
    }
  ],
  "timings": [
    {
      "duration": "2h",
      "time_out_eval": "10m"
    }
  ],
  "results_table_name": "results_simple_choice"
}
