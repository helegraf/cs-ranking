{
  "resources": {
    "gtx1080": "1",
    "ncpus": "2",
    "vmem": "42g",
    "mem": "16g"
  },
  "dataset_configs": [
    {
      "dataset": "synthetic_dc",
      "dataset_params": {
        "dataset_type": "movie_genre",
        "n_objects": 10,
        "n_features": 4,
        "ensure_distance": true,
        "min_distance": 0.2,
        "similarity": 0.01,
        "scatter": true,
        "n_train_instances": 102400,
        "n_test_instances": 10240
      }
    }
  ],
  "folds": [
    {
      "fold_id": 1,
      "n_inner_folds": 0
    }
  ],
  "learning_problem": "discrete_choice",
  "seeds": [
    {
      "seed": 5678
    }
  ],
  "learners": [
    {
      "learner_name": "set_transformer_dc",
      "learner_params": {
        "stacking_height": 1,
        "attention_layer_config": {
          "SAB": {
            "mab": {
              "MAB": {
                "multi_head": {
                  "ScaledDotProductAttention": {
                    "weighted": false,
                    "biased": false
                  }
                },
                "layer_normalization": false,
                "depth_rff": 1,
                "rff_config": {
                  "activation": "selu",
                  "kernel_regularizer":  "l2",
                  "kernel_regularizer_params":  {
                    "l": 1e-08
                  },
                  "use_bias":  true,
                  "bias_initializer":  "zeros"}
              }
            }
          }
        },
        "pooling_config": {
          "PMA": {
            "k": 1,
            "mab": {
              "MAB": {
                "multi_head": {
                  "ScaledDotProductAttention": {
                    "activation": null,
                    "biased": false,
                    "scale": true,
                    "weighted": false,
                    "weights_initializer": "lecun_normal",
                    "factor": null,
                    "sordoni_biased": false
                  }
                }
              }
            },
            "depth_rff": 1,
            "rff_config": {
              "units": 5,
              "activation": "selu",
              "kernel_initializer": "lecun_normal",
              "use_bias": true,
              "bias_initializer": "zeros"
            }
          }
        },
        "num_layers_dense": 1,
        "dense_config": {
          "units": 5,
          "activation": "selu",
          "kernel_initializer": "lecun_normal",
          "use_bias": true,
          "bias_initializer": "zeros"
        },
        "loss_function": "categorical_hinge",
        "loss_function_requires_x_values": false,
        "batch_size": 256,
        "optimizer": "SGD",
        "optimizer_params": {
          "lr": 1e-4,
          "nesterov": true,
          "momentum": 0.9,
          "clipnorm": 1.0
        }
      },
      "learner_fit_params": {
        "epochs": 64,
        "validation_split": 0.1,
        "callbacks": {
          "AdvancedTensorBoard": {
            "num_visualizations_per_epoch": 1,
            "save_space": true,
            "log_lr": true,
            "log_gradient_norms": "global",
            "log_attention": true,
            "no_detailed_histograms": true,
            "lr_vs_accuracy_diagram": true,
            "log_lr_each_batch": true,
            "log_dir": "./tensorboard_logs/",
            "histogram_freq": 1,
            "write_graph": false,
            "write_grads": false,
            "write_images": false,
            "embeddings_freq": 0,
            "update_freq": "epoch"
          },
          "CyclicLR": {
            "base_lr": 1e-10,
            "max_lr": 1e-3,
            "step_size": 25600,
            "mode": "triangular2"
          },
          "EarlyStoppingExtraRestoration": {
            "monitor": "val_loss",
            "min_delta": 0,
            "patience": 16,
            "verbose": 2,
            "mode": "auto",
            "baseline": null,
            "restore_best_weights": true
          }
        }
      }
    }
  ],
  "optimizers": [
    {
      "use_hp": "False",
      "hp_iterations": 0,
      "hp_ranges": {},
      "hp_fit_params": {}
    }
  ],
  "timings": [
    {
      "duration": "2h",
      "time_out_eval": "10m"
    }
  ],
  "results_table_name": "results_simple_dc"
}
