{
  "resources": {
    "gtx1080": "1",
    "ncpus": "2",
    "vmem": "54g",
    "mem": "32g"
  },
  "dataset_configs": [
    {
      "dataset": "synthetic_or",
      "dataset_params": {
        "dataset_type": "simple_max",
        "n_objects": 5,
        "n_train_instances": 102400,
        "n_test_instances": 10240
      }
    }
  ],
  "folds": [
    {
      "fold_id": 1,
      "n_inner_folds": 0
    }
  ],
  "learning_problem": "object_ranking",
  "seeds": [
    {
    "seed": 2345
    },
    {
    "seed": 3456
    },
    {
    "seed": 4567
    },
    {
    "seed": 5678
    },
    {
    "seed": 6789
    },
    {
    "seed": 9876
    },
    {
    "seed": 8765
    },
    {
    "seed": 7654
    },
    {
    "seed": 6543
    }
  ],
  "learners": [
    {
      "learner_name": "feta_ranker",
      "learner_params": {
        "attention_pooling_config": null,
        "attention_preselection_config": null,
        "attention_pooling_flavour": null,
        "num_attention_preselection_layers": 1,
        "n_hidden": 3,
        "n_units": 5,
        "add_zeroth_order_model": true,
        "max_number_of_objects": 5,
        "loss_function": "hinged_rank_loss",
        "loss_function_requires_x_values": false,
        "batch_size": 256,
        "batch_normalization": false,
        "activation": "selu",
        "kernel_initializer": "lecun_normal",
        "kernel_regularizer": "l2",
        "kernel_regularizer_params": {"l": 1e-4},
        "optimizer": "SGD",
        "optimizer_params": {
          "lr": 1e-4,
          "nesterov": "true",
          "momentum": 0.9,
          "clipnorm": 1.0
        }
      },
      "learner_fit_params": {
        "epochs": 128,
        "validation_split": 0.1,
        "callbacks": {
          "AdvancedTensorBoard": {
            "num_visualizations_per_epoch": 0,
            "save_space": true,
            "log_lr": true,
            "log_gradient_norms": "global",
            "log_attention": false,
            "no_detailed_histograms": true,
            "lr_vs_accuracy_diagram": true,
            "log_lr_each_batch": true,
            "log_dir": "./tensorboard_logs/",
            "histogram_freq": 1,
            "write_graph": false,
            "write_grads": false,
            "write_images": false,
            "embeddings_freq": 0,
            "update_freq": "epoch"
          },
          "CyclicLR": {
            "base_lr": 1e-5,
            "max_lr": 1e-1,
            "step_size": 5760,
            "mode": "triangular2"
          },
          "EarlyStoppingExtraRestoration": {
            "monitor": "val_loss",
            "min_delta": 0,
            "patience": 64,
            "verbose": 2,
            "mode": "auto",
            "baseline": null,
            "restore_best_weights": true
          }
        }
      }
    }
  ],
  "optimizers": [
    {
      "use_hp": "False",
      "hp_iterations": 0,
      "hp_ranges": {},
      "hp_fit_params": {}
    }
  ],
  "timings": [
    {
      "duration": "12h",
      "time_out_eval": "10m"
    }
  ],
  "results_table_name": "results_simple_ranking"
}
