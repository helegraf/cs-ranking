{
  "resources": {
    "ncpus": "8",
    "vmem": "42g",
    "mem": "16g"
  },
  "dataset_configs": [
    {
      "dataset": "synthetic_dc",
      "dataset_params": {
        "dataset_type": "movie_genre",
        "n_objects": 10,
        "n_features": 4,
        "ensure_distance": true,
        "min_distance": 0.2,
        "similarity": 0.01,
        "scatter": true,
        "n_train_instances": 102400,
        "n_test_instances": 10240
      }
    }
  ],
  "folds": [
    {
      "fold_id": 1,
      "n_inner_folds": 0
    }
  ],
  "learning_problem": "discrete_choice",
  "seeds": [
    {
      "seed": 1234
    }
  ],
  "learners": [
    {
      "learner_name": "nested_logit_model",
      "learner_params": {
        "n_nests": 2,
        "alpha": 1e-2,
        "loss_function":"categorical_hinge",
        "regularization":"l1"
      },
      "learner_fit_params": {
        "sampler": "variational",
        "tune": 500,
        "draws": 500,
        "cores": 4,
        "vi_params": {
          "n": 20000,
          "method": "advi",
          "callbacks": ["CheckParametersConvergence"]
        }
      }
    }
  ],
  "optimizers": [
    {
      "use_hp": "False",
      "hp_iterations": 0,
      "hp_ranges": {},
      "hp_fit_params": {}
    }
  ],
  "timings": [
    {
      "duration": "6h",
      "time_out_eval": "10m"
    }
  ],
  "results_table_name": "results_simple_dc"
}
