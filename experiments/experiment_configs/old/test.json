{
  "resources": {
    "gtx1080": "1",
    "ncpus": "2",
    "vmem": "42g",
    "mem": "16g"
  },
  "dataset_configs": [
    {
      "dataset": "synthetic_or",
      "dataset_params": {
        "dataset_type": "simple_max",
        "n_objects": 5,
        "n_train_instances": 10000,
        "n_test_instances": 100
      }
    },
    {
      "dataset": "synthetic_or",
      "dataset_params": {
        "dataset_type": "min_max",
        "n_features": 4,
        "n_objects": 5,
        "n_train_instances": 10000,
        "n_test_instances": 100
      }
    }
  ],
  "folds": [
    {
      "fold_id": 1,
      "n_inner_folds": 0
    }
  ],
  "learning_problem": "object_ranking",
  "seeds": [
    {
      "seed": 5678
    }
  ],
  "learners": [ {
      "learner_name": "feta_ranker",
      "learner_params": {
        "attention_pooling_config_config": null,
        "attention_preselection_config": null,
        "attention_pooling_config_flavour": null,
        "num_attention_preselection_layers": 1,
        "n_hidden": 2,
        "n_units": 5,
        "add_zeroth_order_model": true,
        "max_number_of_objects": 5,
        "loss_function": "hinged_rank_loss",
        "loss_function_requires_x_values": false,
        "batch_size": 256,
        "batch_normalization": true,
        "activation": "selu",
        "kernel_initializer": "lecun_normal",
        "kernel_regularizer": "l2",
        "kernel_regularizer_params": {
          "l": 1e-06
        },
        "optimizer": "SGD",
        "optimizer_params": {
          "lr": 0.0001,
          "nesterov": "true",
          "momentum": 0.9
        }
      },
      "learner_fit_params": {
        "epochs": 64,
        "validation_split": 0.1,
        "callbacks": {
          "AdvancedTensorBoard": {
            "num_visualizations_per_epoch": 0,
            "save_space": true,
            "log_lr": true,
            "log_gradient_norms": "global",
            "log_attention": false,
            "no_detailed_histograms": true,
            "lr_vs_accuracy_diagram": true,
            "log_lr_each_batch": true,
            "log_dir": "./tensorboard_logs/",
            "histogram_freq": 1,
            "write_graph": false,
            "write_grads": false,
            "write_images": false,
            "embeddings_freq": 0,
            "update_freq": "epoch"
          },
          "CyclicLR": {
            "base_lr": 1e-10,
            "max_lr": 0.1,
            "step_size": 25600,
            "mode": "triangular2"
          },
          "EarlyStoppingExtraRestoration": {
            "monitor": "val_loss",
            "min_delta": 0,
            "patience": 16,
            "verbose": 2,
            "mode": "auto",
            "baseline": null,
            "restore_best_weights": true
          }
        }
      }
    },
    {
      "learner_name": "set_transformer_ranker",
      "learner_params": {
        "stacking_height": 1,
        "attention_layer_config": {
          "SAB": {
            "mab": {
              "MAB": {
                "multi_head": {
                  "ScaledDotProductAttention": {
                    "activation": null,
                    "biased": false,
                    "scale": true,
                    "weighted": false,
                    "weights_initializer": "lecun_normal",
                    "factor": null,
                    "sordoni_biased": false
                  }
                }
              }
            }
          }
        },
        "num_layers_dense": 2,
        "dense_config": {
          "units": 5,
          "activation": "selu",
          "kernel_regularizer": "l2",
          "kernel_regularizer_params": {
            "l": 1e-4
          },
          "kernel_initializer": "lecun_normal",
          "use_bias": true,
          "bias_initializer": "zeros"
        },
        "loss_function": "hinged_rank_loss",
        "loss_function_requires_x_values": false,
        "batch_size": 256,
        "batch_normalization": false,
        "optimizer": "SGD",
        "optimizer_params": {
          "lr": 1e-4,
          "nesterov": true,
          "momentum": 0.9
        }
      },
      "learner_fit_params": {
        "epochs": 64,
        "validation_split": 0.1,
        "callbacks": {
          "AdvancedTensorBoard": {
            "num_visualizations_per_epoch": 1,
            "save_space": true,
            "log_lr": true,
            "log_gradient_norms": "global",
            "log_attention": true,
            "no_detailed_histograms": true,
            "lr_vs_accuracy_diagram": true,
            "log_lr_each_batch": true,
            "log_dir": "./tensorboard_logs/",
            "histogram_freq": 1,
            "write_graph": false,
            "write_grads": false,
            "write_images": false,
            "embeddings_freq": 0,
            "update_freq": "epoch"
          },
          "CyclicLR": {
            "base_lr": 1e-1,
            "max_lr": 1e3,
            "step_size": 25600,
            "mode": "triangular2"
          },
          "EarlyStopping": {
            "monitor": "val_loss",
            "min_delta": 0,
            "patience": 10,
            "verbose": 2,
            "mode": " auto",
            "baseline": null,
            "restore_best_weights": false
          }
        }
      }
    }
  ],
  "optimizers": [
    {
      "use_hp": "False",
      "hp_iterations": 0,
      "hp_ranges": {},
      "hp_fit_params": {}
    }
  ],
  "timings": [
    {
      "duration": "12h",
      "time_out_eval": "10m"
    }
  ],
  "results_table_name": "results_test"
}
